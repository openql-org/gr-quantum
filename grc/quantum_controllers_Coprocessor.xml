	self.$(id).set_Y_gete_parameters($Y_freq, $Y_I_amp, $Y_Q_amp, $Y_I_bw, $Y_Q_bw, $Y_proc_time, $samp_rate)
	self.$(id).set_Z_gete_parameters($Z_freq, $Z_I_amp, $Z_Q_amp, $Z_I_bw, $Z_Q_bw, $Z_proc_time, $samp_rate)
	self.$(id).set_H_gete_parameters($H_freq, $H_I_amp, $H_Q_amp, $H_I_bw, $H_Q_bw, $H_proc_time, $samp_rate)
	self.$(id).set_T_gete_parameters($T_freq, $T_I_amp, $T_Q_amp, $T_I_bw, $T_Q_bw, $T_proc_time, $samp_rate)
	self.$(id).set_S_gete_parameters($S_freq, $S_I_amp, $S_Q_amp, $S_I_bw, $S_Q_bw, $S_proc_time, $samp_rate)
	self.$(id).set_INIT_parameters($INIT_freq, $INIT_I_amp, $INIT_Q_amp, $INIT_I_bw, $INIT_Q_bw, $INIT_proc_time, $samp_rate)
	self.$(id).set_RO_parameters($RO_freq, $RO_I_amp, $RO_Q_amp, $RO_I_bw, $RO_Q_bw, $RO_proc_time, $samp_rate)
	</make>

	<callback>set_time_scale_rate($scale_rate)</callback>
