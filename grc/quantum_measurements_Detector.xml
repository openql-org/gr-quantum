<?xml version="1.0"?>
<block>
	<name>Qubit Detector</name>
	<key>quantum_measurements_Detector</key>
	<category>[Quantum]/measurements</category>
	<import>import quantum</import>
	<make>quantum.measurements_Detector()</make>

	<param>
		<name>Num Inputs</name>
		<key>num_inputs</key>
		<value>1</value>
		<type>int</type>
	</param>
	<param>
		<name>Show IQ Wave out Port</name>
		<key>showWAVEport</key>
		<value>True</value>
		<type>enum</type>
		<hide>part</hide>
		<option>
			<name>Yes</name>
			<key>False</key>
		</option>
		<option>
			<name>No</name>
			<key>True</key>
		</option>
	</param>

	<sink>
		<name>measurement</name>
		<type>complex</type>
		<nports>$num_inputs</nports>
	</sink>
	<source>
		<name>result</name>
		<type>message</type>
	</source>
	<source>
		<name>feedback</name>
		<type>message</type>
		<optional>1</optional>
	</source>
	<source>
		<name>out</name>
		<type>complex</type>
		<optional>1</optional>
		<hide>$showWAVEport</hide>
	</source>

</block>
